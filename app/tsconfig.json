{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/features": ["src/features"],
      "@/components": ["src/components"],
      "@/stores": ["src/stores"],
      "@/hooks": ["src/hooks"],
      "@/utils": ["src/utils"],
      "@/lib": ["src/lib"],
      "@/generated": ["src/generated"]
    }
  }
}
